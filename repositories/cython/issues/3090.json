        {
            "author_association": "MEMBER",
            "body": "I remember this default was chosen due to non-trivial performance implications. Is the fast calling integration sufficient to overcome this now? ",
            "created_at": "2020-05-22T21:49:55Z",
            "html_url": "https://github.com/cython/cython/issues/3090#issuecomment-632921765",
            "id": 632921765,
            "issue_url": "https://api.github.com/repos/cython/cython/issues/3090",
            "node_id": "MDEyOklzc3VlQ29tbWVudDYzMjkyMTc2NQ==",
            "updated_at": "2020-05-22T21:49:55Z",
            "url": "https://api.github.com/repos/cython/cython/issues/comments/632921765",
            "user": {
                "avatar_url": "https://avatars3.githubusercontent.com/u/486017?v=4",
                "events_url": "https://api.github.com/users/robertwb/events{/privacy}",
                "followers_url": "https://api.github.com/users/robertwb/followers",
