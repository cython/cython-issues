        {
            "author_association": "NONE",
            "body": "In C++, `extern \"C++\"` is the default, but in this case, Cython overrides it with `extern \"C\"`.",
            "created_at": "2017-10-15T06:44:54Z",
            "html_url": "https://github.com/cython/cython/issues/1839#issuecomment-336690406",
            "id": 336690406,
            "issue_url": "https://api.github.com/repos/cython/cython/issues/1839",
            "node_id": "MDEyOklzc3VlQ29tbWVudDMzNjY5MDQwNg==",
            "performed_via_github_app": null,
